":\"none\"}' label=\"招聘要求\" prop=\"zhaopinyaoqiu\">\r\n            <el-input v-model=\"ruleForm.zhaopinyaoqiu\" \r\n                placeholder=\"招聘要求\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"企业账号\" prop=\"qiyezhanghao\">\r\n            <el-input v-model=\"ruleForm.qiyezhanghao\" \r\n                placeholder=\"企业账号\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"企业名称\" prop=\"qiyemingcheng\">\r\n            <el-input v-model=\"ruleForm.qiyemingcheng\" \r\n                placeholder=\"企业名称\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"联系手机\" prop=\"lianxishouji\">\r\n            <el-input v-model=\"ruleForm.lianxishouji\" \r\n                placeholder=\"联系手机\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"发布日期\" prop=\"faburiqi\" >\r\n              <el-date-picker\r\n                  format=\"yyyy 年 MM 月 dd 日\"\r\n                  value-format=\"yyyy-MM-dd\"\r\n                  v-model=\"ruleForm.faburiqi\" \r\n                  type=\"date\"\r\n                  placeholder=\"发布日期\">\r\n              </el-date-picker> \r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"招聘简介\" prop=\"zhaopinjianjie\">\r\n            <el-input\r\n              type=\"textarea\"\r\n              :rows=\"8\"\r\n              placeholder=\"招聘简介\"\r\n              v-model=\"ruleForm.zhaopinjianjie\">\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"招聘内容\" prop=\"zhaopinneirong\">\r\n            <editor \r\n                :style='{\"minHeight\":\"300px\",\"padding\":\"0\",\"margin\":\"0\",\"borderColor\":\"#ccc\",\"backgroundColor\":\"#fff\",\"borderRadius\":\"0\",\"borderWidth\":\"0\",\"width\":\"100%\",\"borderStyle\":\"solid\",\"height\":\"auto\"}'\r\n                v-model=\"ruleForm.zhaopinneirong\" \r\n                class=\"editor\" \r\n                action=\"file/upload\">\r\n            </editor>\r\n          </el-form-item>\r\n\r\n      <el-form-item :style='{\"padding\":\"0\",\"margin\":\"20px 0 20px 0\"}'>\r\n        <el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0\",\"boxShadow\":\"1px 2px 3px #aaa\",\"margin\":\"0 20px 0 0\",\"color\":\"#fff\",\"outline\":\"none\",\"borderRadius\":\"4px\",\"background\":\"linear-gradient(180deg, rgba(17,112,210,1) 0%, rgba(8,179,68,1) 100%)\",\"width\":\"128px\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\",\"height\":\"40px\"}'  type=\"primary\" @click=\"onSubmit\">提交</el-button>\r\n        <el-button :style='{\"border\":\"1px solid #bbb\",\"cursor\":\"pointer\",\"padding\":\"0\",\"boxShadow\":\"1px 2px 6px #ccc\",\"margin\":\"0\",\"color\":\"#999\",\"outline\":\"none\",\"borderRadius\":\"4px\",\"background\":\"#fff\",\"width\":\"128px\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\",\"height\":\"40px\"}' @click=\"back()\">返回</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        id: '',\r\n        baseUrl: '',\r\n        ro:{\r\n            zhaopinmingcheng : false,\r\n            zhaopintupian : false,\r\n            zhiweimingcheng : false,\r\n            xinchoudaiyu : false,\r\n            zhaopinjianjie : false,\r\n            zhaopinneirong : false,\r\n            zhaopinyaoqiu : false,\r\n            qiyezhanghao : false,\r\n            qiyemingcheng : false,\r\n            lianxishouji : false,\r\n            faburiqi : false,\r\n            clicktime : false,\r\n        },\r\n        type: '',\r\n        userTableName: localStorage.getItem('UserTableName'),\r\n        ruleForm: {\r\n          zhaopinmingcheng: '',\r\n          zhaopintupian: '',\r\n          zhiweimingcheng: '',\r\n          xinchoudaiyu: '',\r\n          zhaopinjianjie: '',\r\n          zhaopinneirong: '',\r\n          zhaopinyaoqiu: '',\r\n          qiyezhanghao: '',\r\n          qiyemingcheng: '',\r\n          lianxishouji: '',\r\n          faburiqi: '',\r\n          clicktime: '',\r\n        },\r\n        zhiweimingchengOptions: [],\r\n        rules: {\r\n          zhaopinmingcheng: [\r\n            { required: true, message: '招聘名称不能为空', trigger: 'blur' },\r\n          ],\r\n          zhaopintupian: [\r\n          ],\r\n          zhiweimingcheng: [\r\n          ],\r\n          xinchoudaiyu: [\r\n          ],\r\n          zhaopinjianjie: [\r\n          ],\r\n          zhaopinneirong: [\r\n          ],\r\n          zhaopinyaoqiu: [\r\n          ],\r\n          qiyezhanghao: [\r\n          ],\r\n          qiyemingcheng: [\r\n          ],\r\n          lianxishouji: [\r\n          ],\r\n          faburiqi: [\r\n          ],\r\n          clicktime: [\r\n          ],\r\n        },\r\n      };\r\n    },\r\n    computed: {\r\n\r\n\r\n\r\n    },\r\n    created() {\r\n\t  //this.bg();\r\n      let type = this.$route.query.type ? this.$route.query.type : '';\r\n      this.init(type);\r\n      this.baseUrl = this.$config.baseUrl;\r\n      this.ruleForm.faburiqi = this.getCurDate()\r\n    },\r\n    methods: {\r\n      getMakeZero(s) {\r\n          return s < 10 ? '0' + s : s;\r\n      },\r\n      // 下载\r\n      download(file){\r\n        window.open(`${file}`)\r\n      },\r\n      // 初始化\r\n      init(type) {\r\n        this.type = type;\r\n        if(type=='cross'){\r\n          var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n          for (var o in obj){\r\n            if(o=='zhaopinmingcheng'){\r\n              this.ruleForm.zhaopinmingcheng = obj[o];\r\n              this.ro.zhaopinmingcheng = true;\r\n              continue;\r\n            }\r\n            if(o=='zhaopintupian'){\r\n              this.ruleForm.zhaopintupian = obj[o].split(\",\")[0];\r\n              this.ro.zhaopintupian = true;\r\n              continue;\r\n            }\r\n            if(o=='zhiweimingcheng'){\r\n              this.ruleForm.zhiweimingcheng = obj[o];\r\n              this.ro.zhiweimingcheng = true;\r\n              continue;\r\n            }\r\n            if(o=='xinchoudaiyu'){\r\n              this.ruleForm.xinchoudaiyu = obj[o];\r\n              this.ro.xinchoudaiyu = true;\r\n              continue;\r\n            }\r\n            if(o=='zhaopinjianjie'){\r\n              this.ruleForm.zhaopinjianjie = obj[o];\r\n              this.ro.zhaopinjianjie = true;\r\n              continue;\r\n            }\r\n            if(o=='zhaopinneirong'){\r\n              this.ruleForm.zhaopinneirong = obj[o];\r\n              this.ro.zhaopinneirong = true;\r\n              continue;\r\n            }\r\n            if(o=='zhaopinyaoqiu'){\r\n              this.ruleForm.zhaopinyaoqiu = obj[o];\r\n              this.ro.zhaopinyaoqiu = true;\r\n              continue;\r\n            }\r\n            if(o=='qiyezhanghao'){\r\n              this.ruleForm.qiyezhanghao = obj[o];\r\n              this.ro.qiyezhanghao = true;\r\n              continue;\r\n            }\r\n            if(o=='qiyemingcheng'){\r\n              this.ruleForm.qiyemingcheng = obj[o];\r\n              this.ro.qiyemingcheng = true;\r\n              continue;\r\n            }\r\n            if(o=='lianxishouji'){\r\n              this.ruleForm.lianxishouji = obj[o];\r\n              this.ro.lianxishouji = true;\r\n              continue;\r\n            }\r\n            if(o=='faburiqi'){\r\n              this.ruleForm.faburiqi = obj[o];\r\n              this.ro.faburiqi = true;\r\n              continue;\r\n            }\r\n            if(o=='clicktime'){\r\n              this.ruleForm.clicktime = obj[o];\r\n              this.ro.clicktime = true;\r\n              continue;\r\n            }\r\n          }\r\n        }\r\n        // 获取用户信息\r\n        this.$http.get(this.userTableName + '/session', {emulateJSON: true}).then(res => {\r\n          if (res.data.code == 0) {\r\n            var json = res.data.data;\r\n            if((json.qiyezhanghao!=''&&json.qiyezhanghao) || json.qiyezhanghao==0){\r\n                this.ruleForm.qiyezhanghao = json.qiyezhanghao\r\n            }\r\n            if((json.qiyemingcheng!=''&&json.qiyemingcheng) || json.qiyemingcheng==0){\r\n                this.ruleForm.qiyemingcheng = json.qiyemingcheng\r\n            }\r\n            if((json.lianxishouji!=''&&json.lianxishouji) || json.lianxishouji==0){\r\n                this.ruleForm.lianxishouji = json.lianxishouji\r\n            }\r\n          }\r\n        });\r\n        this.$http.get('option/zhiweimingcheng/zhiweimingcheng', {emulateJSON: true}).then(res => {\r\n          if (res.data.code == 0) {\r\n            this.zhiweimingchengOptions = res.data.data;\r\n          }\r\n        });\r\n      },\r\n\r\n    // 多级联动参数\r\n      // 多级联动参数\r\n      info(id) {\r\n        this.$http.get('zhaopinxinxi/detail/${id}', {emulateJSON: true}).then(res => {\r\n          if (res.data.code == 0) {\r\n            this.ruleForm = res.data.data;\r\n          }\r\n        });\r\n      },\r\n      // 提交\r\n      onSubmit() {\r\n\r\n        //更新跨表属性\r\n        var crossuserid;\r\n        var crossrefid;\r\n        var crossoptnum;\r\n        this.$refs[\"ruleForm\"].validate(valid => {\r\n          if(valid) {\r\n            if(this.type=='cross'){\r\n                 var statusColumnName = localStorage.getItem('statusColumnName');\r\n                 var statusColumnValue = localStorage.getItem('statusColumnValue');\r\n                 if(statusColumnName && statusColumnName!='') {\r\n                     var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n                     if(!statusColumnName.startsWith(\"[\")) {\r\n                         for (var o in obj){\r\n                             if(o==statusColumnName){\r\n                                 obj[o] = statusColumnValue;\r\n                             }\r\n                         }\r\n                         var table = localStorage.getItem('crossTable');\r\n                         this.$http.post(table+'/update', obj).then(res => {});\r\n                     } else {\r\n                            crossuserid=Number(localStorage.getItem('userid'));\r\n                            crossrefid=obj['id'];\r\n                            crossoptnum=localStorage.getItem('statusColumnName');\r\n                            crossoptnum=crossoptnum.replace(/\\[/,\"\").replace(/\\]/,\"\");\r\n                     }\r\n                 }\r\n            }\r\n            if(crossrefid && crossuserid) {\r\n                 this.ruleForm.crossuserid=crossuserid;\r\n                 this.ruleForm.crossrefid=crossrefid;\r\n                 var params = {\r\n                     page: 1,\r\n                     limit: 10,\r\n                     crossuserid:crossuserid,\r\n                     crossrefid:crossrefid,\r\n                 }\r\n                 this.$http.get('zhaopinxinxi/list', {\r\n              