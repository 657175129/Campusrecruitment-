\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"企业账号\" prop=\"qiyezhanghao\">\r\n            <el-input v-model=\"ruleForm.qiyezhanghao\" \r\n                placeholder=\"企业账号\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"企业名称\" prop=\"qiyemingcheng\">\r\n            <el-input v-model=\"ruleForm.qiyemingcheng\" \r\n                placeholder=\"企业名称\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"联系手机\" prop=\"lianxishouji\">\r\n            <el-input v-model=\"ruleForm.lianxishouji\" \r\n                placeholder=\"联系手机\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"账号\" prop=\"zhanghao\">\r\n            <el-input v-model=\"ruleForm.zhanghao\" \r\n                placeholder=\"账号\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"姓名\" prop=\"xingming\">\r\n            <el-input v-model=\"ruleForm.xingming\" \r\n                placeholder=\"姓名\" clearable ></el-input>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0